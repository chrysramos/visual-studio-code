// Place your key bindings in this file to overwrite the defaults
[
    { "key": "ctrl+Shift+d", "command": "editor.action.copyLinesDownAction", "when": "editorTextFocus" },
    { "key": "F12", "command": "extension.open", "when": "editorTextFocus" },
    { "key": "ctrl+Shift+c",  "command": "extension.colorHelper.pick", "when": "editorTextFocus" },
    { "key": "ctrl+;", "command": "editor.action.addCommentLine", "when": "editorTextFocus && !editorReadonly" }
]
